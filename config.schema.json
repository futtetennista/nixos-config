{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "@@backups.passwords.bear@@": {
      "type": "string"
    },
    "@@networking.computerNamePrefix@@": {
      "type": "string"
    },
    "@@programs.git.userName@@": {
      "type": "string"
    },
    "@@programs.git.userEmail@@": {
      "type": "string",
      "format": "email"
    },
    "@@proton.email@@": {
      "type": "string"
    },
    "@@github.user@@": {
      "type": "string"
    },
    "@@nixCache.url@@": {
      "type": "string",
      "format": "uri"
    },
    "@@nixCache.publicKey@@": {
      "type": "string"
    },
    "@@ssh.key.path@@": {
      "type": "string"
    },
    "@@system.user@@": {
      "type": "string"
    },
    "@@user.signingKey@@": {
      "type": "string"
    }
  },
  "required": [
    "@@backups.passwords.bear@@",
    "@@github.user@@",
    "@@networking.computerNamePrefix@@",
    "@@nixCache.publicKey@@",
    "@@nixCache.url@@",
    "@@programs.git.userEmail@@",
    "@@programs.git.userName@@",
    "@@proton.email@@",
    "@@ssh.key.path@@",
    "@@system.user@@",
    "@@user.signingKey@@"
  ]
}
